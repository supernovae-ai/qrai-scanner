{
  "name": "@qrcodeai/qraisc-scanner",
  "version": "0.1.0",
  "description": "High-performance QR code validation and scannability scoring",
  "main": "index.js",
  "types": "index.d.ts",
  "author": "Thibaut @ SuperNovae Studio",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/SuperNovae-studio/qraisc-scanner"
  },
  "keywords": [
    "qrcode",
    "qr",
    "validation",
    "scanner",
    "rust",
    "napi"
  ],
  "napi": {
    "name": "qraisc-scanner",
    "triples": {
      "defaults": true,
      "additional": [
        "aarch64-apple-darwin",
        "aarch64-unknown-linux-gnu",
        "aarch64-unknown-linux-musl"
      ]
    }
  },
  "devDependencies": {
    "@napi-rs/cli": "^2.18.0"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --platform --release",
    "build:debug": "napi build --platform",
    "prepublishOnly": "napi prepublish -t npm",
    "test": "node test.js",
    "universal": "napi universal",
    "version": "napi version"
  },
  "engines": {
    "node": ">= 16"
  }
}
