{
  "name": "@supernovae-st/qrcode-ai-scanner",
  "version": "0.2.1",
  "description": "High-performance QR code validation and scannability scoring",
  "main": "index.js",
  "types": "index.d.ts",
  "author": "Thibaut @ SuperNovae Studio",
  "license": "AGPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/supernovae-st/qrcode-ai-scanner"
  },
  "keywords": [
    "qrcode",
    "qr",
    "validation",
    "scanner",
    "rust",
    "napi"
  ],
  "napi": {
    "name": "qrcode-ai-scanner",
    "triples": {
      "defaults": true,
      "additional": [
        "aarch64-apple-darwin",
        "aarch64-unknown-linux-gnu",
        "aarch64-unknown-linux-musl"
      ]
    }
  },
  "devDependencies": {
    "@napi-rs/cli": "^2.18.0"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --platform --release",
    "build:debug": "napi build --platform",
    "prepublishOnly": "napi prepublish -t npm",
    "test": "node test-ok.mjs",
    "universal": "napi universal",
    "version": "napi version"
  },
  "engines": {
    "node": ">= 16"
  },
  "optionalDependencies": {
    "@supernovae-st/qrcode-ai-scanner-win32-x64-msvc": "0.2.1",
    "@supernovae-st/qrcode-ai-scanner-darwin-x64": "0.2.1",
    "@supernovae-st/qrcode-ai-scanner-linux-x64-gnu": "0.2.1",
    "@supernovae-st/qrcode-ai-scanner-darwin-arm64": "0.2.1",
    "@supernovae-st/qrcode-ai-scanner-linux-arm64-gnu": "0.2.1",
    "@supernovae-st/qrcode-ai-scanner-linux-arm64-musl": "0.2.1"
  }
}